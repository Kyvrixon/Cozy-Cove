{
	"name": "cozy-cove",
	"version": "0.0.1",
	"type": "module",
	"author": "Kyvrixon",
	"dependencies": {
		"colorette": "^2.0.20",
		"discord.js": "^14.19.3",
		"dotenv": "^16.5.0",
		"mongoose": "^8.16.0",
		"node-cron": "^4.1.0"
	},
	"scripts": {
		"format": "npx prettier --write --log-level=warn .",
		"lint": "npx eslint --fix --quiet .",
		"build": "npx tsc",
		"start": "npm run run",
		"test": "npx tsc && node ./dist/src/test.js",
		"run": "cross-env NODE_ENV=production node --optimize-for-size --max-old-space-size=128 --gc-interval=100 ./dist/src/index.js",
		"dev": "npm i --force && npm run build && node ./dist/src/index.js",
		"stripe": "npm run build && node ./dist/src/stripe/init.js"
	},
	"devDependencies": {
		"@eslint/js": "^9.29.0",
		"@types/node": "^22.15.32",
		"@types/node-cron": "^3.0.11",
		"cross-env": "^7.0.3",
		"eslint": "^9.29.0",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.0"
	}
}
